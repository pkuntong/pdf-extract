{
  "name": "PDF Invoice Extractor",
  "short_name": "PDF Extractor",
  "description": "Upload PDF invoices and extract structured data instantly. Export to CSV format.",
  "theme_color": "#1e40af",
  "background_color": "#111827",
  "display": "standalone",
  "orientation": "portrait",
  "scope": "/",
  "start_url": "/",
  "id": "/",
  "categories": ["productivity", "business", "utilities"],
  "icons": [
    {
      "src": "/icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/pdf": [".pdf"]
      }
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "params": {
      "files": [
        {
          "name": "pdf_files",
          "accept": ["application/pdf", ".pdf"]
        }
      ]
    }
  },
  "shortcuts": [
    {
      "name": "Upload PDF",
      "short_name": "Upload",
      "description": "Upload a new PDF for data extraction",
      "url": "/",
      "icons": [{ "src": "/icon-192x192.png", "sizes": "192x192" }]
    }
  ]
}